{
  "name": "miniPaint",
  "version": "4.14.2",
  "author": "Vilius L.",
  "description": "Online graphics editing tool lets create, edit images using HTML5 technologies.",
  "keywords": [
    "canvas",
    "drawing",
    "paint",
    "layers",
    "effects"
  ],
  "scripts": {
    "server": "webpack serve --mode development --env development --open",
    "server:test": "webpack serve --mode development --env development --port 8080",
    "server:react": "webpack serve --mode development --env react --open --port 8081",
    "server:react:test": "webpack serve --mode development --env react --port 8081",
    "dev": "webpack --mode development",
    "dev:react": "webpack --mode development --env react",
    "dev:all": "webpack --mode development --env all",
    "build": "webpack --mode production",
    "build:react": "webpack --mode production --env react",
    "build:all": "webpack --mode production --env all",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "playwright test",
    "test:e2e:react": "TEST_REACT=true playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:all": "npm run test && npm run test:e2e"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/viliusle/miniPaint"
  },
  "homepage": "https://github.com/viliusle/miniPaint",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.14.5",
    "@babel/plugin-transform-runtime": "^7.14.5",
    "@babel/preset-env": "^7.28.5",
    "@babel/preset-react": "^7.28.5",
    "@playwright/test": "^1.57.0",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/react": "^16.3.1",
    "babel-jest": "^30.2.0",
    "babel-loader": "^8.2.2",
    "css-loader": "^5.2.6",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "jsdom": "^27.4.0",
    "source-map-loader": "^3.0.0",
    "style-loader": "^2.0.0",
    "webpack": "^5.76.0",
    "webpack-cli": "^4.7.2",
    "webpack-dev-server": "^4.3.1"
  },
  "dependencies": {
    "@babel/runtime": "^7.14.5",
    "alertifyjs": "^1.13.1",
    "blueimp-canvas-to-blob": "^3.28.0",
    "exif-js": "^2.3.0",
    "file-saver": "^2.0.5",
    "fuzzysort": "^1.1.4",
    "gif.js.optimized": "^1.0.1",
    "hermite-resize": "git+https://github.com/viliusle/Hermite-resize.git",
    "jquery": "^3.5.1",
    "pica": "^7.0.0",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "semver-compare": "^1.0.0",
    "uuid": "^8.3.2",
    "webfontloader": "^1.6.28"
  }
}
